#!/bin/bash -e

  runtimelog=$1

  submitList=$(cat $runtimelog | grep "New Job" | rev | cut -c 2-4 | rev)
  hostList=$(cat $runtimelog | grep "Target host" | rev | cut -c -2 | rev)

  numcharBEGIN=1
  numcharEND=2
  while  read -r line; do
    separator=$(echo $line | cut -c 1)
    if [ "$separator" != ":" ]; then 
       if [ "$separator2" != " " ]; then
          #Task with 3 numbers
          task=$line
       else
          #Task with 2 numbers
          task=$(echo $line | cut -c 2-)
       fi 
    else
       #Task with 1 number 
       task=$(echo $line | cut -c 3-)
    fi

    resource=$(echo $hostList | cut -c $numcharBEGIN-$numcharEND)
    echo "$task $resource"
    numcharBEGIN=$((numcharBEGIN+3))
    numcharEND=$((numcharEND+3))
  done <<< "$submitList"

