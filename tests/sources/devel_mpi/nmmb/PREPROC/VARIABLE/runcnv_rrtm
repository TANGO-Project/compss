rm flist
ls -1 ../output/*.gfs > flist
ifort  -traceback -assume byterecl -O3 -fp-model precise -fp-stack-check -mcmodel=large -shared-intel -convert big_endian -c inc_rrtm.f
ifort  -traceback -assume byterecl -O3 -fp-model precise -fp-stack-check -mcmodel=large -shared-intel -convert big_endian -o inc_rrtm.x inc_rrtm.o 
./inc_rrtm.x
rm *.o
rm *.x
ifort  -traceback -assume byterecl -O3 -fp-model precise -fp-stack-check -mcmodel=large -shared-intel -convert big_endian -c cnv_rrtm.f
ifort  -traceback -assume byterecl -O3 -fp-model precise -fp-stack-check -mcmodel=large -shared-intel -convert big_endian -o cnv_rrtm.x cnv_rrtm.o
./cnv_rrtm.x
rm *.o
rm *.x

rm flist
